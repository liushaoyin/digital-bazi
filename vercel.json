{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
    { "src": "/(.*\\.(?:css|js|html|png|jpg|jpeg|gif|svg))", "dest": "/$1" },
    { "src": "/api/.*", "dest": "server.js" },
    { "src": "/admin", "dest": "server.js" },
    { "src": "/", "dest": "server.js" }
    ]
} 